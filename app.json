      }
    },
    "android": {
      "package": "com.alwaysgood.app",
      "googleServicesFile": "./keys/google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB7zLRgccH8E9oNBmtME_lan7zAkI2P3ds"
        }
      },
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "version": "1.0.0",
    "facebookAppId": "2609459805809503",
    "facebookScheme": "fb2609459805809503",
    "facebookDisplayName": "Always Good"
  }
