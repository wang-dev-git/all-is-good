        "NSPhotoLibraryUsageDescription": "Add a profile picture to your account",
        "NSCameraUsageDescription": "Take a picture to add to your profile",
        "CFBundleURLTypes": {
          "CFBundleURLSchemes": ["fb2609459805809503"]
        },
        "FacebookAppId": "2609459805809503",
        "FacebookDisplayName": "AlwaysGood",
